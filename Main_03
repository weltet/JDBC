import java.util.ArrayList;

public class Main {
	public static void main(String[] args) {
		DBManager manager = new DBManager();
		
		try {
			ArrayList<Student> result = manager.getAllData();
			
			for(Student tmp : result) {
				System.out.printf("%d, %s, %s, %s, %d\n", 
								tmp.getId(),tmp.getName(),tmp.getAddress(), tmp.getEmail(), tmp.getKor());
			}
		
		}catch(Exception e) {
			e.printStackTrace();
		}
	}
}
